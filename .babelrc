{
  "presets": [
    [
      "@babel/env",
      {
        "modules": false, //默认都是支持 CommonJS
        "targets": {
          "browsers": [
            "last 3 Chrome versions",
            "last 3 Firefox versions",
            "Safari >= 10",
            "Explorer >= 11",
            "Edge >= 12",
            "iOS >= 10",
            "Android >= 6"
          ]
        }
      }
    ]
  ],
  "comments": false //不产生注释
}
